{
  "version": 2,
  "name": "enterprise-cia",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-url.railway.app/api/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://your-backend-url.railway.app",
    "NEXT_PUBLIC_WS_URL": "wss://your-backend-url.railway.app"
  }
}
